<div class="container">
  <h1 class="text-center">All artworks</h1>
  <br>
  <div class="row">

      <% if @artworks.present? %>
        <%= render partial: 'components/artwork',collection: @artworks %>
      <% else %>
        <p>There are no posts containing the term(s) <%= params[:search] %>.</p>
      <% end %>

  </div>
</div>



